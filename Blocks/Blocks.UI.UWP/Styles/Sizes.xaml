<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Blocks.UI.Uwp.Styles">
    <Style x:Key="FlagStripStyle" TargetType="Rectangle">
        <Setter Property="Height" Value="22"/>
        <Setter Property="Width" Value="88"/>
    </Style>

    <Style x:Key="FlagFootStyle" TargetType="Rectangle">
        <Setter Property="Height" Value="176"/>
        <Setter Property="Width" Value="11"/>
    </Style>

    <x:Double x:Key="FlagStripHeight">22</x:Double>
    <x:Double x:Key="FlagStripHeightDoubled">44</x:Double>
    <x:Double x:Key="FlagStripWidth">88</x:Double>
    <x:Double x:Key="FlagFootHeight">176</x:Double>
    <x:Double x:Key="FlagFootWidth">11</x:Double>

    <GridLength x:Key="FlagStripGridWidth" >88</GridLength>
    <GridLength x:Key="FlagStripGridHeight">22</GridLength>
    <GridLength x:Key="FlagFootGridWidth" >11</GridLength>
    <GridLength x:Key="FlagFootGridHeight">176</GridLength>
    <GridLength x:Key="FlagFootLeftGridHeight">110</GridLength>
    
</ResourceDictionary>
